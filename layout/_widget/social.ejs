<div class="chireiden-widget-wrap">
    <div class="chireiden-widget chireiden-social">
        <% for (name in theme.social) { %><a class="mdui-ripple" href="<%- url_for(theme.social[name][0]) -%>" target="_blank" mdui-tooltip="{content: '<%= name %>'}" style="color: <%= theme.social[name][2] %>;background-color: <%= theme.social[name][3] %>;">
            <i class="chireidenfont <% if (theme.social[name][1]){ %><%= theme.social[name][1] %><%} else{ %><%= 'icon-unorderedlist' %><% } %>"></i>
        </a><% } %>
    </div>
</div>